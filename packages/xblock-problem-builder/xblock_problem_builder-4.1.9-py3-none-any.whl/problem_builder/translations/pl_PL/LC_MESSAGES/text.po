#: message.py:57 message.py:178
msgid "Message (Incomplete)"
msgstr "Wiadomość (Nie ukończono)"

#: message.py:58
msgid "Message shown when incomplete"
msgstr "Wiadomość wyświetlana, gdy nie ukończono"

#: message.py:59
