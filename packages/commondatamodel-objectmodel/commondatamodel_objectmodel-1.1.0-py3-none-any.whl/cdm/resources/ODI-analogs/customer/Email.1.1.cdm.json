{
  "documentVersion": "1.1",
  "jsonSchemaSemanticVersion": "1.0.0",
  "$schema": "../../schema.cdm.json",
  "imports": [
    {
      "corpusPath": "_allImports.1.1.cdm.json"
    }
  ],
  "definitions": [
    {
      "entityName": "Email",
      "extendsEntity": "ODIEntity",
      "displayName": "Email",
      "description": "An email type of interaction.",
      "exhibitsTraits": [
        "means.ODI.interaction.emailMessage",
        {
          "traitReference": "is.ODI.equivalent.entity",
          "arguments": [
            {
              "name": "equivalentOf",
              "value": "urn://odi.org/v1-beta/entities/customer/Email"
            }
          ]
        },
        {
          "traitReference": "is.CDM.entityVersion",
          "arguments": [
            {
              "name": "versionNumber",
              "value": "1.1"
            }
          ]
        }
      ],
      "hasAttributes": [
        {
          "name": "emailInteractionEvent",
          "displayName": "Interaction Event Identity",
          "description": "Unique identifier of the interaction across all interaction types",
          "entity": {
            "entityReference": "InteractionEvent",
            "appliedTraits": [
              {
                "traitReference": "is.identifiedBy",
                "arguments": [
                  "InteractionEvent/(resolvedAttributes)/idValue"
                ]
              }
            ]
          },
          "appliedTraits": [
            {
              "traitReference": "is.ODI.equivalent.entityAttribute",
              "arguments": [
                {
                  "name": "equivalentOf",
                  "value": "urn://odi.org/v1-beta/entities/customer/emailInteractionEvent"
                }
              ]
            }
          ],
          "resolutionGuidance": {
            "entityByReference": {
              "allowReference": true,
              "foreignKeyAttribute": {
                "purpose": "hasA",
                "dataType": "entityId",
                "name": "id",
                "appliedTraits": [
                  "is.ODI.attribute.required"
                ]
              }
            }
          }
        },
        {
          "purpose": "hasA",
          "dataType": "string",
          "name": "conversationId",
          "displayName": "Conversation Identity",
          "description": "Identifies the conversation thread of which many email may be a part.",
          "appliedTraits": [
            "is.ODI.attribute.required",
            {
              "traitReference": "is.ODI.equivalent.attribute",
              "arguments": [
                {
                  "name": "equivalentOf",
                  "value": "urn://odi.org/v1-beta/entities/customer/conversationId"
                }
              ]
            }
          ],
          "isNullable": true
        },
        {
          "purpose": "hasA",
          "dataType": "dateTime",
          "name": "sentDateTime",
          "displayName": "Sent Date Time",
          "description": "The actual date and time when the message was sent",
          "appliedTraits": [
            "is.ODI.attribute.required",
            {
              "traitReference": "is.ODI.equivalent.attribute",
              "arguments": [
                {
                  "name": "equivalentOf",
                  "value": "urn://odi.org/v1-beta/entities/customer/sentDateTime"
                }
              ]
            }
          ]
        },
        {
          "purpose": "hasA",
          "dataType": "dateTime",
          "name": "receivedDateTime",
          "displayName": "Date Time Received",
          "description": "The actual date and time when the message was received",
          "appliedTraits": [
            "is.ODI.attribute.required",
            {
              "traitReference": "is.ODI.equivalent.attribute",
              "arguments": [
                {
                  "name": "equivalentOf",
                  "value": "urn://odi.org/v1-beta/entities/customer/receivedDateTime"
                }
              ]
            }
          ]
        },
        {
          "purpose": "hasA",
          "dataType": "string",
          "name": "emailBody",
          "displayName": "Body",
          "description": "A description about the objective or primary topic of the interaction.",
          "appliedTraits": [
            "is.ODI.attribute.required",
            {
              "traitReference": "is.ODI.equivalent.attribute",
              "arguments": [
                {
                  "name": "equivalentOf",
                  "value": "urn://odi.org/v1-beta/entities/customer/emailBody"
                }
              ]
            }
          ],
          "isNullable": true
        },
        {
          "purpose": "hasA",
          "dataType": "string",
          "name": "emailSubject",
          "displayName": "Subject",
          "description": "The subject of the interaction.",
          "appliedTraits": [
            "is.ODI.attribute.required",
            {
              "traitReference": "is.ODI.equivalent.attribute",
              "arguments": [
                {
                  "name": "equivalentOf",
                  "value": "urn://odi.org/v1-beta/entities/customer/emailSubject"
                }
              ]
            }
          ],
          "isNullable": true
        },
        {
          "purpose": "hasA",
          "dataType": "boolean",
          "name": "hasAttachments",
          "displayName": "Has Attachments",
          "description": "Indicates if the email message has attached documents.",
          "appliedTraits": [
            "is.ODI.attribute.required",
            {
              "traitReference": "is.ODI.equivalent.attribute",
              "arguments": [
                {
                  "name": "equivalentOf",
                  "value": "urn://odi.org/v1-beta/entities/customer/hasAttachments"
                }
              ]
            }
          ],
          "isNullable": true
        },
        {
          "purpose": "hasA",
          "dataType": "string",
          "name": "importance",
          "displayName": "Importance",
          "description": "An importance or priority level applied to the email.",
          "appliedTraits": [
            "is.ODI.attribute.required",
            {
              "traitReference": "is.ODI.equivalent.attribute",
              "arguments": [
                {
                  "name": "equivalentOf",
                  "value": "urn://odi.org/v1-beta/entities/customer/importance"
                }
              ]
            }
          ],
          "isNullable": true
        }
      ]
    }
  ]
}