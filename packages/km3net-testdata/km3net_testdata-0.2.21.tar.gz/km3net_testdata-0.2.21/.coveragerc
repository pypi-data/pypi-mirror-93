[run]
source = km3net_testdata

[report]
include =
    km3net_testdata/*
omit =
    tests/*
    km3net_testdata/version.py
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if self.debug:
    if settings.DEBUG
    def __repr__
