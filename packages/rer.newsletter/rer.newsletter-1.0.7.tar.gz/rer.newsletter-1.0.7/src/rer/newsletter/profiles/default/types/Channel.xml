<?xml version="1.0"?>
<object name="Channel" meta_type="Dexterity FTI" i18n:domain="rer.newsletter"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Channel</property>
 <property name="description" i18n:translate=""></property>
 <property name="factory">Channel</property>
 <property name="add_view_expr">string:${folder_url}/++add++Channel</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
   <element value="Message" />
   <element value="Folder" />
   <element value="Collection" />
   <element value="Shippable Collection" />
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">folder_listing</property>
 <property name="view_methods">
  <element value="folder_listing" />
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">rer.newsletter.AddChannel</property>
 <property name="klass">rer.newsletter.content.channel.Channel</property>
 <property name="behaviors">
   <element value="plone.app.dexterity.behaviors.metadata.IDublinCore"/>
   <element value="plone.app.content.interfaces.INameFromTitle"/>
   <element value="plone.app.dexterity.behaviors.discussion.IAllowDiscussion"/>
   <element value="plone.app.dexterity.behaviors.exclfromnav.IExcludeFromNavigation"/>
   <element value="plone.app.dexterity.behaviors.id.IShortName"/>
   <element value="Products.CMFPlone.interfaces.constrains.ISelectableConstrainTypes"/>
   <element value="plone.app.relationfield.behavior.IRelatedItems"/>
   <element value="plone.app.dexterity.behaviors.nextprevious.INextPreviousToggle"/>
 </property>
 <property name="schema">rer.newsletter.interfaces.IChannelSchema</property>
 <property name="model_source"></property>
 <property name="model_file"></property>
 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
