[Unit]
Description=Daphne server for SITENAME

[Service]
Restart=on-failure
User=USERNAME
Environment=EMAIL_PASSWORD=SECRET SMTP_EMAL_PASSWORD=SMTP_SECRET
WorkingDirectory=/root/SITENAME/sources
ExecStart=/home/USERNAME/.local/bin/daphne -b 0 --access-log ../accsee.log -p 8001 PROJECT_DIR_NAME.asgiSUFFIX:application

[Install]
WantedBy=multi-user.target
