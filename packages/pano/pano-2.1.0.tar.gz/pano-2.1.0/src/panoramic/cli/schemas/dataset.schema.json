{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://panoramichq.com/dataset.schema.json",
    "title": "Dataset",
    "description": "Groups models in a queryable collection",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "api_version": {
            "description": "Version of the Model schema",
            "type": "string",
            "enum": [
                "v1"
            ]
        },
        "dataset_slug": {
            "description": "Slug for dataset",
            "type": "string"
        },
        "display_name": {
            "description": "Name for dataset",
            "type": "string"
        }
    },
    "required": [
        "dataset_slug",
        "display_name"
    ]
}
