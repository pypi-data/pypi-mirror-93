<?xml version="1.0"?>
<odoo>

    <template id="assets_backend"
              name="barcodes_ean14 Assets"
              inherit_id="web.assets_backend"
              >
        <xpath expr="." position="inside">
            <script type="application/javascript"
                    src="/barcodes_ean14/static/src/js/barcode_parser.js"
                    />
        </xpath>
    </template>

    <template id="qunit_suite"
              name="barcodes_ean14 QUnit Assets"
              inherit_id="web.qunit_suite"
              >
        <xpath expr="//t" position="inside">
            <script type="application/javascript"
                    src="/barcodes_ean14/static/tests/js/barcode_parser.js"
                    />
        </xpath>
    </template>

</odoo>
