#!/home/payno/.local/share/virtualenvs/est_venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'est==0.3.0b1','console_scripts','est'
__requires__ = 'est==0.3.0b1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('est==0.3.0b1', 'console_scripts', 'est')()
    )
