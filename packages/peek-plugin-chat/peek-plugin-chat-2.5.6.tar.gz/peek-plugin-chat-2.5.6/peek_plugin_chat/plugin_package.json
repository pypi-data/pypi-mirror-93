{
    "plugin": {
        "title": "Chat",
        "packageName": "peek_plugin_chat",
    "version":"2.5.6",
        "buildNumber": "#PLUGIN_BUILD#",
        "buildDate": "#BUILD_DATE#",
        "creator": "Synerty Pty Ltd",
        "website": "www.synerty.com"
    },
    "requiresServices": [
        "server",
        "client",
        "storage",
        "admin",
        "mobile",
        "doc-dev",
        "doc-admin",
        "doc-user"
