<?xml version="1.0"?>
<wps:Execute xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" version="1.0.0" service="WPS">
  <ows:Identifier>removeJob</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>job_id</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{ job_id }}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:RawDataOutput>
      <ows:Identifier>is_removed</ows:Identifier>
    </wps:RawDataOutput>
  </wps:ResponseForm>
</wps:Execute>
